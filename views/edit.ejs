


<% include navbar.ejs %>
<div class="container mt-5">
<form action="/users/<%= data.id %>?_method=PUT" method="POST">
    <input type="hidden" name="_method" value="PUT">
        <div class="form-group">
          <label for="exampleInputEmail1">Employee Name</label>
          <input type="text" class="form-control" id="exampleInputEmail1" name="username" aria-describedby="emailHelp" placeholder="Employee Name"  value="<%= data.username %>">
        </div>
        <div class="form-group">
          <label for="exampleInputPassword1">Employee Surname</label>
          <input type="text" class="form-control" name="lastName" placeholder="Employee Surname"  value="<%= data.lastName %>"> 
        </div>
        <button type="submit" class="btn btn-primary">Update</button>
      </form>
</div>
